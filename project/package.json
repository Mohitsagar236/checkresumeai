{
    "name":  "checkresumeai",
    "version":  "1.0.0",
    "private":  true,
    "type":  "module",    "scripts":  {
                    "dev":  "node --max-http-header-size=32768 ./node_modules/vite/bin/vite.js",
                    "dev:safe":  "cross-env NODE_OPTIONS=\"--max-http-header-size=32768\" vite",
                    "prebuild": "npm install",
                    "build":  "node scripts/railway-build.js",
                    "build:frontend": "vite build --mode production --minify esbuild",
                    "build:backend": "cd backend && npm install && npm run build",
                    "build:optimized":  "GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true vite build --mode production --minify esbuild",
                    "simple-vercel-build":  "node scripts/simple-vercel-build.js",
                    "render-build":  "node scripts/render-build.js",
                    "ultra-fast-build":  "node scripts/ultra-fast-build.js",
                    "setup-pdf-worker":  "echo \u0027Setting up PDF worker files...\u0027",
                    "vercel-build":  "vite build --mode production --minify esbuild --emptyOutDir",
                    "start":  "cd backend && node dist/server.js",
                    "health-check":  "node scripts/health-check.js"
                },
    "devDependencies":  {
                            "@vitejs/plugin-react":  "^4.5.2",
                            "autoprefixer":  "^10.4.21",
                            "cross-env":  "^7.0.3",
