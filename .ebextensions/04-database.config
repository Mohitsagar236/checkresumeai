option_settings:
  aws:rds:dbinstance:
    DBInstanceClass: db.t3.micro
    DBEngine: mysql
    DBEngineVersion: 8.0.41
    DBAllocatedStorage: 10
    DBUser: checkresumeai
    MultiAZDatabase: false
    DBDeletionPolicy: Snapshot
  aws:elasticbeanstalk:application:environment:
    # Database connection will be automatically configured by EB
    DB_HOST: 
      "Fn::GetAtt": ["AWSEBRDSDatabase", "Endpoint.Address"]
    DB_PORT: 
      "Fn::GetAtt": ["AWSEBRDSDatabase", "Endpoint.Port"]
