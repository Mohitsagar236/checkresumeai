files:
  "/etc/nginx/conf.d/gzip.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
      # Enable gzip compression
      gzip on;
      gzip_comp_level 6;
      gzip_vary on;
      gzip_min_length 1000;
      gzip_proxied any;
      gzip_disable "msie6";
      gzip_types
        text/plain
        text/css
        text/xml
        text/javascript
        application/javascript
        application/xml+rss
        application/json
        application/atom+xml
        image/svg+xml
        application/x-font-ttf
        font/opentype;
